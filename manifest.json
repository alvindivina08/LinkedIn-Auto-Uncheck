{
  "manifest_version": 2,
  "name": "LinkedIn AutoFill Extension",
  "version": "1.0",
  "description": "Automatically fills in your name and phone number on LinkedIn forms.",
  "permissions": [
      "storage",
      "activeTab",
      "https://www.linkedin.com/*"
  ],
  "content_scripts": [
      {
          "matches": ["https://www.linkedin.com/*"],
          "js": ["content.js"]
      }
  ],
  "options_page": "options.html"
}
